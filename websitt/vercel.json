{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "mossy.co" }],
      "destination": "https://usemossy.co/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.mossy.co" }],
      "destination": "https://usemossy.co/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.usemossy.co" }],
      "destination": "https://usemossy.co/:path*",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "trailingSlash": false
}
